// -*- mode: jsonc -*-
{
    "layer": "top",
    "position": "top",
    "height": 30,
    "spacing": 0,
    "reload_style_on_change": true,
    "modules-left": [
        "clock",
        "custom/battery",
        "custom/pipewire",
        "wireplumber"
    ],
    "modules-center": [
        "hyprland/workspaces"
    ],
    "modules-right": [
        "idle_inhibitor",
        "bluetooth",
        "network",
        "cpu",
        "memory",
        "disk",
        "custom/power"
    ],
    "include": [
        "$HOME/.config/waybar/modules.jsonc"
    ]
}
